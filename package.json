{
  "name": "nuxtjs.org",
  "version": "1.1.0",
  "description": "Nuxt.js official website",
  "main": "index.js",
  "dependencies": {
    "@nuxt/content": "nuxt/content#dev",
    "@nuxt/http": "^0.5.9",
    "@nuxt/static": "^1.0.0",
    "@nuxtjs/tailwindcss": "2.0.0",
    "@snackbar/core": "^1.7.0",
    "clipboard": "^2.0.6",
    "cookie": "^0.4.1",
    "defu": "^2.0.4",
    "degit": "^2.8.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "glob": "^7.1.6",
    "intersection-observer": "^0.11.0",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "micro": "^9.3.4",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.13.3",
    "nuxt-i18n": "^6.13.1",
    "nuxt-svg-loader": "^1.2.0",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "reading-time": "^1.2.0",
    "v-lazy-image": "^1.4.0",
    "vue-observe-visibility": "^0.4.6",
    "vue-scrollactive": "^0.9.3",
    "vue-scrollto": "^2.18.1"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "^1.0.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/netlify-files": "^1.2.0",
    "@nuxtjs/proxy": "^2.0.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.10.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "markdownlint-cli": "^0.23.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "start-server-and-test": "^1.11.2",
    "stylelint": "^13.6.1"
  },
  "scripts": {
    "dev:api:docs": "nodemon modules/docs/dev.js",
    "dev": "nuxt",
    "build": "nuxt build",
    "build:check": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt-static && cypress install --force && npm run test:e2e:run",
    "serve": "nuxt serve",
    "lintold": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern modules/ --ignore-pattern plugins/ga.js .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:run": "start-server-and-test serve http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern modules/ --ignore-pattern plugins/ga.js .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"content/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/nuxtjs.org.git"
  },
  "keywords": [
    "nuxt.js",
    "vuejs",
    "nextjs",
    "js"
  ],
  "author": "alexchopin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxt/nuxtjs.org/issues"
  },
  "homepage": "https://github.com/nuxt/nuxtjs.org#readme",
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
